{
  "name": "who-died-when",
  "author": {
    "name": "Ali Zahid",
    "email": "hi@alizahid.dev",
    "url": "https://alizahid.dev"
  },
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "yarn css:build; remix build",
    "css:build": "postcss styles --base styles --dir app/styles --env production",
    "css:watch": "postcss styles --base styles --dir app/styles -w",
    "db": "env-cmd -f .env.production prisma",
    "dev": "concurrently 'yarn css:watch' 'env-cmd remix dev'",
    "lint": "eslint . --fix",
    "postinstall": "remix setup node",
    "prepare": "husky install",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@prisma/client": "^3.5.0",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "algoliasearch": "^4.11.0",
    "axios": "^0.24.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-imgix": "^9.3.0",
    "remix": "^1.0.6",
    "tailwind-merge": "^0.9.0",
    "use-onclickoutside": "^0.4.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@tailwindcss/typography": "^0.4.1",
    "@types/listr": "^0.14.4",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-imgix": "^9.3.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": "^7.0.4",
    "listr": "^0.14.3",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2",
    "prettier": "^2.5.0",
    "pretty-quick": "^3.1.2",
    "prisma": "^3.5.0",
    "tailwindcss": "^2.2.19",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
